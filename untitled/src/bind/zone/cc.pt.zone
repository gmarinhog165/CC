;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	Servidor1.cc.pt. root.Servidor1.cc.pt. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	Servidor1.cc.pt.
	IN	NS	Servidor2.cc.pt.
; name servers - A records
Servidor1.cc.pt.	IN	A	10.4.4.1
Servidor2.cc.pt.	IN	A	10.4.4.2

; 10.0.0.0/24 - A records
Portatil1.cc.pt.	IN	A	10.1.1.1
Portatil2.cc.pt.	IN	A	10.1.1.2
PC1.cc.pt.	IN	A	10.2.2.1
PC2.cc.pt.	IN	A	10.2.2.2
Roma.cc.pt.	IN	A	10.3.3.1
Paris.cc.pt.	IN	A	10.3.3.2
